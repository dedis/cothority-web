{
  "name": "cothority-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dedis/cothority": "^1.1.10",
    "ajv": "^6.4.0",
    "bootstrap": "^4.1.0",
    "bytebuffer": "^5.0.1",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "node-sass": "^4.9.0",
    "prop-types": "^15.6.1",
    "protobufjs": "^6.8.6",
    "react": "^16.3.2",
    "react-custom-scrollbars": "^4.0.2",
    "react-dom": "^16.3.2",
    "react-fontawesome": "^1.5.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scripts": "^1.1.4",
    "reactstrap": "^5.0.0",
    "split-host": "^0.1.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "argv-parser": "^0.1.4",
    "faker": "^4.1.0",
    "inliner": "^1.12.1",
    "mock-socket": "^6.0.4",
    "npm-run-all": "^4.0.2"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "npm test --env=jsdom -- --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!**/lib/**",
    "eject": "react-scripts eject"
  }
}
